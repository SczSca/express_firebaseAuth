<!-- TODO: agregar un table head -->
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Usuario</th>
            <th scope="col">Acceso</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < users.length; i++){ %>
            <tr>
                <th scope="row"><%= users[i].email %></th>
                <td>
                    <div class="form-check form-switch">
                        <input class="form-check-input" type="checkbox" role="switch" id="<%= users[i].uid %>" <%= (users[i].customClaims.access) ? 'checked' : users[i].customClaims %> >
                    </div>
                </td>
            </tr>
            
        <% } %>
    </tbody>
</table>
